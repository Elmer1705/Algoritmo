           si n1 >= 10 y n1 <= 50 entonces ;
                  Escribir "Tendr치s un descuento del 5%. ";
      finsi 
           sino 
                si n1 >= 51 y n1 <= 100 entonces ;
                        Escribir "Tendr치s un descuento del 10%. ";
           finsi
